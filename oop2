#include <iostream> //1
using namespace std;
int main()
{
    float num;
    int n=1, k = 1;
    cout<<"Введите число для таблицы умножения "<<endl;
    cin>>num;
    while(k < 200){
        cout<<num*k;
        k++;
        if(n==10){
            cout<<endl;
            n=0;
        }
        n++;
    }
    return 0;
}
